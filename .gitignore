# =========================================
# Laravel default ignores
# =========================================
/vendor
/node_modules
/.phpunit.cache
/public/build
/public/hot
/.env
/.env.* 
Homestead.json
Homestead.yaml
npm-debug.log
yarn-error.log
/.idea
/.vscode
/bootstrap/cache/*.php

# =========================================
# Ignore all storage except reports
# =========================================
/storage/*
!/storage/app/public/reports/
!.gitignore

# =========================================
# Optional: keep public storage symlink ignored
# =========================================
/public/storage/*
